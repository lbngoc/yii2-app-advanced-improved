{
    "name": "mghollander/yii2-app-advanced-improved",
    "description": "Improved Yii 2 Advanced Project Template",
    "keywords": ["yii2", "framework", "advanced", "project template", "improved"],
    "homepage": "https://github.com/MGHollander/yii2-app-advanced-improved/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/MGHollander/yii2-app-advanced-improved/issues?state=open",
        "wiki": "https://github.com/MGHollander/yii2-app-advanced-improved/wiki",
        "source": "https://github.com/MGHollander/yii2-app-advanced-improved/"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": ">=2.0.6",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
